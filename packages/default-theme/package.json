{
  "name": "@blognode/default-theme",
  "version": "0.0.1",
  "main": "dist/server/index",
  "scripts": {
    "build": "concurrently npm:build:pages npm:build:server npm:build:main",
    "build:pages": "webpack -c webpack-pages.config.js --mode=production --node-env=production",
    "build:server": "webpack -c webpack-server.config.js --mode=production --node-env=production",
    "build:main": "webpack --mode=production --node-env=production",
    "watch": "concurrently npm:watch:pages npm:watch:server npm:watch:main",
    "watch:pages": "webpack -c webpack-pages.config.js --mode=development --node-env=development -w",
    "watch:server": "webpack -c webpack-server.config.js --mode=development --node-env=development -w",
    "watch:main": "webpack-dev-server -c webpack.config.js --mode=development"
  },
  "dependencies": {
    "@blognode/renderer-react": "*",
    "axios": "^0.27.2",
    "blognode": "*",
    "bootstrap": "^5.2.0",
    "han-css": "^3.3.0",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.2.0",
    "remixicon": "^2.5.0"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/webpack-env": "^1.17.0",
    "@webpack-cli/generators": "^2.5.0",
    "autoprefixer": "^10.4.7",
    "css-loader": "^6.7.1",
    "esbuild-loader": "^2.19.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-node-externals": "^3.0.0"
  }
}
