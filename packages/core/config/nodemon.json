{
  "delay": 1000,
  "watch": ["src"],
  "execMap": {
    "ts": "node --no-warnings --experimental-import-meta-resolve --experimental-specifier-resolution=node --loader ts-paths-esm-loader",
    "js": "ts-node -r tsconfig-paths/register"
  },
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,ts"
}
