<!DOCTYPE html>
<html lang="<%= pageLang %>">
<head>
  <meta charset="UTF-8" />
  <title><%= pageTitle %></title>
  <% linkTags.forEach(link=> { -%>
  <link rel="<%- link.rel %>" type="<%= link.type %>" href="<%= link.href %>">
  <% }) -%>
  <% scriptTags.forEach(script=> { -%>
  <script src="<%- script.src %>" <%= script.async ? "async":"" %> <%= script.defer ? "defer":"" %>></script>
  <% }) -%>
  <%- pageHead -%>
</head>
<body>
  <%- pageBody -%>
  <script id="__blognode-pagedata__" type="text/json"><%- JSON.stringify(pageCtx) %></script>
  <script id="__pagename" type="text/plain"><%- pageName %></script>
</body>
</html>